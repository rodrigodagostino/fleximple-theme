		"postcss-color-mod-function": "3.0.3",
		"postcss-loader": "6.2.1",
		"postcss-preset-env": "7.4.3",
		"postcss-rem": "2.0.2",
		"sass": "1.49.9",
		"sass-loader": "12.6.0",
		"thread-loader": "3.0.4",
		"webpack": "5.70.0",
		"webpack-cli": "4.9.2"
