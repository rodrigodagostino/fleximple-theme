{
	"name": "fleximple-theme",
	"version": "0.5.0",
	"description": "The “Fleximple Theme” designed for WordPress",
	"main": "build/scripts.js",
	"scripts": {
		"build": "wp-scripts build",
		"check-engines": "wp-scripts check-engines",
		"check-licenses": "wp-scripts check-licenses",
		"lint:css": "wp-scripts lint-style",
		"lint:css:src": "wp-scripts lint-style 'src/**/*.css'",
		"lint:js": "wp-scripts lint-js",
		"lint:js:src": "wp-scripts lint-js ./src",
		"lint:pkg-json": "wp-scripts lint-pkg-json",
		"makejson": "wp i18n make-json languages/fleximpletheme-es_ES.po --no-purge",
		"makepot": "wp i18n make-pot ./ languages/fleximpletheme.pot",
		"start": "wp-scripts start",
		"test:e2e": "wp-scripts test-e2e",
		"test:unit": "wp-scripts test-unit-js"
	},
	"keywords": [
		"fleximple",
		"wordpress",
		"theme"
	],
	"author": "Rodrigo D’Agostino",
	"license": "ISC",
	"browserslist": [
		"last 2 versions",
		"ie >= 9",
		"ios >= 7"
	],
	"devDependencies": {
		"@babel/core": "^7.14.6",
		"@babel/plugin-proposal-class-properties": "^7.14.5",
		"@wordpress/babel-plugin-makepot": "^3.10.0",
		"@wordpress/babel-preset-default": "^4.20.0",
		"@wordpress/scripts": "^12.6.1",
		"animate.css": "^4.1.1",
		"array-move": "^3.0.1",
		"babel-eslint": "^10.1.0",
		"css-loader": "^5.2.7",
		"eslint": "^7.31.0",
		"fibers": "^5.0.0",
		"file-loader": "^6.2.0",
		"ignore-emit-webpack-plugin": "^2.0.6",
		"memize": "^1.1.0",
		"mini-css-extract-plugin": "^1.6.2",
		"node-sass": "^4.14.1",
		"path": "^0.12.7",
		"postcss": "^8.3.5",
		"postcss-at-rules-variables": "^0.3.0",
		"postcss-loader": "^4.2.0",
		"postcss-preset-env": "^6.7.0",
		"postcss-rem": "^1.1.5",
		"sass": "^1.35.2",
		"sass-loader": "^10.1.1"
	}
}
