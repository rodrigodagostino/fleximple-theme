		"@wordpress/dependency-extraction-webpack-plugin": "^3.2.1",
		"animate.css": "^4.1.1",
		"clean-webpack-plugin": "^4.0.0-alpha.0",
		"css-loader": "^6.2.0",
		"eslint": "^8.7.0",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-react": "^7.24.0",
		"fibers": "^5.0.0",
		"file-loader": "^6.2.0",
