{
	"name": "fleximple-theme",
	"version": "1.0.1",
	"description": "The “Fleximple Theme” designed for WordPress",
	"main": "dist/scripts.js",
	"scripts": {
		"start": "webpack --mode=development --watch",
		"build": "webpack --mode=production",
		"makejson": "wp i18n make-json languages/es_ES.po --no-purge",
		"makepot": "wp i18n make-pot ./ languages/fleximpletheme.pot"
	},
	"keywords": [
		"fleximple",
		"wordpress",
		"theme"
	],
	"author": "Rodrigo D’Agostino",
	"license": "ISC",
	"browserslist": [
		"last 2 versions",
		"ie >= 11"
	],
	"devDependencies": {
		"@babel/eslint-parser": "7.17.0",
		"@babel/runtime": "7.17.8",
		"@wordpress/babel-preset-default": "6.7.0",
		"@wordpress/dependency-extraction-webpack-plugin": "3.4.1",
		"animate.css": "4.1.1",
		"babel-loader": "8.2.4",
		"clean-webpack-plugin": "4.0.0",
		"css-loader": "6.7.1",
		"eslint": "8.12.0",
		"eslint-plugin-jsx-a11y": "6.5.1",
		"eslint-plugin-react": "7.29.4",
		"fibers": "5.0.1",
		"file-loader": "6.2.0",
		"ignore-emit-webpack-plugin": "2.0.6",
		"memize": "1.1.0",
